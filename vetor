algoritmo "vetor"

var contador:inteiro numeros: vetor[1..5] de inteiro

inicio

para contador <- 1 ate 5 faca escreva("Informe o Número para ser Armazenado: ") leia(numeros[contador]) fimpara

para contador <- 1 ate 5 faca escreval("O valor digitado na posição ", contador, " do vetor foi: ", numeros[contador]) fimpara

fimalgoritmo
